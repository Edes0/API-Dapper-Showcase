{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "ColorConsole": {
      "LogLevels": {
        "Information": "DarkGreen",
        "Warning": "Cyan",
        "Error": "Red"
      }
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DbConnectionString": "${{ secrets.AZURE_CONNECTIONSTRING }}"
  },
  "SystemTime": {
    "Timezone": ""
  },
  "AzureAd": {
    "Instance": "${{ secrets.AZUREAD_INSTANCE }}",
    "ClientId": "${{ secrets.AZUREAD_CLIENTID }}",
    "TenantId": "${{ secrets.AZUREAD_TENANTID }}",
    "Audience": "${{ secrets.AZUREAD_AUDIENCE }}",
    "Issuer": "${{ secrets.AZUREAD_ISSUER }}"
  }
}
